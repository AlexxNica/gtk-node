{
  "name": "node-gtk3",
  "version": "0.0.2",
  "description": "Node bindings for Gtk3",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "build": "node ./node_modules/nwb/lib/bin/nwb build-web-module",
    "clean": "node ./node_modules/nwb/lib/bin/nwb  clean-module",
    "demo": "node ./node_modules/babel-cli/bin/babel-node ./demo/src/index.js",
    "test": "node ./node_modules/nwb/lib/bin/nwb  test",
    "test:coverage": "node ./node_modules/nwb/lib/bin/nwb  test --coverage",
    "test:watch": "node ./node_modules/nwb/lib/bin/nwb  test --server",
    "prepublish": "make build"
  },
  "dependencies": {
    "ffi": "^2.2.0"
  },
  "devDependencies": {
    "nwb": "^0.18.10"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  },
  "author": "",
  "homepage": "",
  "license": "MIT",
  "repository": ""
}
